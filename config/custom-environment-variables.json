{
  "openapiConfig": {
    "filePath": "OPENAPI_FILE_PATH"
  },
  "telemetry": {
    "serviceName": "TELEMETRY_SERVICE_NAME",
    "hostname": "TELEMETRY_HOST_NAME",
    "version": "TELEMETRY_SERVICE_VERSION",
    "logger": {
      "level": "LOG_LEVEL",
      "prettyPrint": {
        "__name": "LOG_PRETTY_PRINT_ENABLED",
        "__format": "boolean"
      }
    },
    "tracing": {
      "enabled": "TELEMETRY_TRACING_ENABLED",
      "url": "TELEMETRY_TRACING_URL"
    },
    "metrics": {
      "enabled": "TELEMETRY_METRICS_ENABLED",
      "url": "TELEMETRY_METRICS_URL",
      "interval": "TELEMETRY_METRICS_INTERVAL"
    }
  },
  "server": {
    "port": "SERVER_PORT",
    "request": {
      "payload": {
        "limit": "REQUEST_PAYLOAD_LIMIT"
      }
    },
    "response": {
      "compression": {
        "enabled": {
          "__name": "RESPONSE_COMPRESSION_ENABLED",
          "__format": "boolean"
        }
      }
    }
  },
  "splitAttempts": {
    "__name": "SPLIT_ATTEMPTS",
    "__format": "number"
  },
  "sourceMountPath": "SOURCE_MOUNT_PATH",
  "tiles": {
    "path": "TILES_PATH"
  },
  "vrt": {
    "outputDirPath": "VRT_OUTPUT_DIR_PATH",
    "sourcesListFilePath": "VRT_SOURCES_LIST_FILE_PATH",
    "nodata": "VRT_NODATA",
    "outputSRS": "VRT_OUTPUT_SRS",
    "resampling": "VRT_RESAMPLING",
    "addAlpha": {
      "__name": "VRT_ADD_ALPHA",
      "__format": "boolean"
    },
    "useBandCount": {
      "__name": "VRT_USE_BAND_COUNT",
      "__format": "boolean"
    },
    "inputBandCount": {
      "__name": "VRT_INPUT_BAND_COUNT",
      "__format": "number"
    }
  },
  "generateTiles": {
    "resampling": "GENERATE_TILES_RESMAPLING",
    "profile": "GENERATE_TILES_PROFILE",
    "srcnodata": "GENERATE_TILES_SRC_NODATA",
    "verbose": {
      "__name": "GENERATE_TILES_VERBOSE",
      "__format": "boolean"
    }
  },
  "queue": {
    "jobManagerBaseUrl": "QUEUE_JOB_MANAGER_BASE_URL",
    "heartbeatManagerBaseUrl": "QUEUE_HEART_BEAT_MANAGER_BASE_URL",
    "dequeueIntervalMs": {
      "__name": "QUEUE_DEQUEUE_INTERVAL_MS",
      "__format": "number"
    },
    "heartbeatIntervalMs": {
      "__name": "QUEUE_HEART_BEAT_INTERVAL_MS",
      "__format": "number"
    },
    "jobType": "QUEUE_JOB_TYPE",
    "tilesTaskType": "QUEUE_TASK_TYPE"
  },
  "storageProvider": "STORAGE_PROVIDER",
  "S3": {
    "accessKeyId": "AWS_ACCESS_KEY_ID",
    "secretAccessKey": "AWS_SECRET_ACCESS_KEY",
    "endpoint": "AWS_S3_ENDPOINT",
    "bucket": "S3_BUCKET",
    "forcePathStyle": {
      "__name": "S3_FORCE_PATH_STYLE",
      "__format": "boolean"
    },
    "sslEnabled": {
      "__name": "AWS_HTTPS",
      "__format": "boolean"
    },
    "virtualHosting": {
      "__name": "AWS_VIRTUAL_HOSTING",
      "__format": "boolean"
    }
  },
  "overseerUrl": "DISCRETE_OVERSEER_URL"
}
