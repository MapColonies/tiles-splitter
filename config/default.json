{
  "openapiConfig": {
    "filePath": "./openapi3.yaml",
    "basePath": "/docs",
    "rawPath": "/api",
    "uiPath": "/api"
  },
  "telemetry": {
    "logger": {
      "level": "info",
      "prettyPrint": false
    }
  },
  "server": {
    "port": "8080",
    "request": {
      "payload": {
        "limit": "1mb"
      }
    },
    "response": {
      "compression": {
        "enabled": true,
        "options": null
      }
    }
  },
  "splitAttempts": 5,
  "sourceMountPath": "/app",
  "vrt": {
    "outputDirPath": "/app/vrtOutputs",
    "sourcesListFilePath": "/app/vrtOutputs/files.txt"
  },
  "tiles": {
    "path": "/path/to/tiles/directory"
  },
  "queue": {
    "jobManagerBaseUrl": "http://10.45.130.11:8088",
    "heartbeatManagerBaseUrl": "http://localhost:8083",
    "dequeueIntervalMs": 1000,
    "heartbeatIntervalMs": 300,
    "jobType": "Ingestion",
    "tilesTaskType": "splitTilesSync"
  },
  "httpRetry": {
    "attempts": 5,
    "delay": "exponential",
    "shouldResetTimeout": true
  },
  "storageProvider": "s3",
  "S3": {
    "accessKeyId": "minioadmin",
    "secretAccessKey": "minioadmin",
    "endpoint": "http://localhost:9000",
    "bucket": "mapping-images",
    "forcePathStyle": true
  }
}
