{
  "openapiConfig": {
    "filePath": "./openapi3.yaml",
    "basePath": "/docs",
    "rawPath": "/api",
    "uiPath": "/api"
  },
  "telemetry": {
    "logger": {
      "level": "info",
      "prettyPrint": false
    }
  },
  "server": {
    "port": "8080",
    "request": {
      "payload": {
        "limit": "1mb"
      }
    },
    "response": {
      "compression": {
        "enabled": true,
        "options": null
      }
    }
  },
  "splitAttempts": 5,
  "sourceMountPath": "/app",
  "vrt": {
    "outputDirPath": "/app/vrtOutputs",
    "sourcesListFilePath": "/app/vrtOutputs/files.txt",
    "nodata": "0",
    "outputSRS": "EPSG:4326",
    "resampling": "average",
    "addAlpha": true
  },
  "generateTiles": {
    "resampling": "average",
    "profile": "geodetic",
    "srcnodata": "0",
    "verbose": true
  },
  "tiles": {
    "path": "/app/vol2"
  },
  "queue": {
    "jobManagerBaseUrl": "http://10.45.130.11:8088",
    "heartbeatManagerBaseUrl": "http://localhost:8083",
    "dequeueIntervalMs": 1000,
    "heartbeatIntervalMs": 300,
    "jobType": "Ingestion",
    "tilesTaskType": "splitTilesSync"
  },
  "httpRetry": {
    "attempts": 5,
    "delay": "exponential",
    "shouldResetTimeout": true
  },
  "storageProvider": "FS",
  "S3": {
    "accessKeyId": "minioadmin",
    "secretAccessKey": "minioadmin",
    "endpoint": "http://10.45.130.11:9000",
    "bucket": "dtm",
    "forcePathStyle": true,
    "sslEnabled": false,
    "virtualHosting": false
  }
}
